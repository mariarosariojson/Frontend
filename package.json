  "type": "module",
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "scripts": {
    "start": "npm run devserver",
    "start:chrome": "npm run devserver -- --open-app-name=chrome",
    "start:firefox": "npm run devserver -- --open-app-name=firefox",
    "start:edge": "npm run devserver -- --open-app-name=msedge",
    "devserver": "webpack serve --mode=development",
    "devservervscode": "npm run devserver -- --env vscodeDebug",
    "predevserver": "pwsh -NoProfile -ExecutionPolicy Unrestricted -Command ./check-npm.ps1",
    "build": "webpack --mode=production",
    "build:test": "webpack --mode=production --env environment=test",
    "build:test:dev": "webpack --mode=development --env environment=test",
    "build:dev": "webpack --mode=development",
    "analyze:prod": "npm run build -- --env bundleAnalyzer",
    "analyze:dev": "npm run build:dev -- --env bundleAnalyzer",
    "eslint": "eslint src/**/*.ts{,x}",
    "eslint:fix": "eslint --fix src/**/*.ts{,x} --cache",
    "eslint:fix:imports": "eslint --fix src/**/*.ts{,x} --cache --rule \"unused-imports/no-unused-imports-ts: 1\" && npm run prettier:fix",
    "eslint:fix:fragments": "eslint --fix src/**/*.ts{,x} --cache --rule \"react/jsx-no-useless-fragment: 1\" && npm run prettier:fix",
    "prettier": "prettier --check src/**/*.ts{,x}",
    "prettier:fix": "prettier --write src/**/*.ts{,x}",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "stylelint --fix src/**/*.css",
    "deletemodules": "rimraf node_modules",
    "deletemodules:win": "rmdir /q /s node_modules",
    "backend": "dotnet run --project ../Backend",
    "serve": "npx browser-sync start --server dist --watch --port 8085",
    "s1": "node webpack-dev-server.js --mode=development",
    "s2": "npm run s1 -- --vscodeDebug",
    "pres1": "pwsh -NoProfile -ExecutionPolicy Unrestricted -Command ./check-npm.ps1"
  },
